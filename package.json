{
  "name": "pii-documentation-generator",
  "displayName": "pii-documentation-generator",
  "description": "",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.75.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:extension.showTextField",
    "onCommand:extension.callParserNConverter",
    "onCommand:extension.gptAPI",
    "onCommand:extension.createProjectInfoFile"
  ],
  "main": "./extension.js",
  "contributes": {
    "commands": [
      {
        "command": "pii-documentation-generator.helloWorld",
        "title": "Hello World"
      }
    ],
    "keybindings": [
      {
        "command": "extension.showTextField",
        "key": "ctrl+h",
        "mac": "cmd+h",
        "linux": "ctrl+h"
      },
      {
        "command": "extension.callParserNConverter",
        "key": "ctrl+shift+h",
        "mac": "cmd+shift+h",
        "linux": "ctrl+shift+h"
      },
      {
        "command": "extension.gptAPI",
        "key": "ctrl+g",
        "mac": "cmd+g",
        "linux": "ctrl+g"
      },
      {
        "command": "extension.createProjectInfoFile",
        "key": "ctrl+shift+i",
        "mac": "cmd+shift+i",
        "linux": "ctrl+shift+i"
      }
    ]
  },
  "scripts": {
    "lint": "eslint .",
    "pretest": "npm run lint",
    "test": "node ./test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^8.1.0",
    "@types/mocha": "^10.0.1",
    "@types/node": "16.x",
    "@types/vscode": "^1.75.0",
    "@vscode/test-electron": "^2.2.3",
    "eslint": "^8.34.0",
    "glob": "^8.1.0",
    "mocha": "^10.2.0",
    "typescript": "^4.9.5"
  },
  "dependencies": {
    "dedent": "^0.7.0",
    "dedent-js": "^1.0.1",
    "prettier": "^2.8.7",
    "dotenv": "^16.0.3",
    "openai": "^3.2.1"
  }
}
